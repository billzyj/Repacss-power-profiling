# REPACSS Power Measurement Configuration Template
# Copy this file to .env and fill in your actual values

# Database Settings
REPACSS_DB_HOST=localhost
REPACSS_DB_PORT=5432
REPACSS_DB_DEFAULT=h100
REPACSS_DB_USER=your_database_user
REPACSS_DB_PASSWORD=your_database_password
REPACSS_DB_SSL_MODE=prefer

# SSH Settings
REPACSS_SSH_HOSTNAME=your.ssh.host.com
REPACSS_SSH_PORT=22
REPACSS_SSH_USERNAME=your_ssh_username
REPACSS_SSH_KEY_PATH=/path/to/your/private/key
REPACSS_SSH_PASSPHRASE=
REPACSS_SSH_KEEPALIVE=60
